<h1>Listagem de usuários</h1>


<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Nome</th>
            <th scope="col">Ocupação</th>
            <th scope="col">Promoções</th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        {{#each users}}
        <tr>
            <th scope="row">{{this.id}}</th>
            <td>{{this.name}}</td>
            <td>{{this.occupation}}</td>
            <td>{{this.newsletter}}</td>
            <td></td>
        </tr>
        {{/each}}
    </tbody>
</table>